              ]
    /{userId}:
      uriParameters:
        userId:
          description: Id of the user.
          type: integer
      type: base
      get:
        description: Check public membership.
        responses:
          204:
            description:  User is a public member.
          404:
            description:  User is not a public member.
      put:
        description: Publicize a user's membership.
        responses:
